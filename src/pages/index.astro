---
import Layout from '@layouts/Layout.astro';
import Particles from '@components/Particles/Particles.vue';
import Hero from '@/components/Hero/Hero.astro';
import Background from '@/components/Background/Background.astro';
import Projects from '@/components/Projects/Projects.astro';
import siteData from '@config/siteData.json';
---

<Layout title={siteData.title} description={siteData.description}>
  <Particles client:only="vue" />
  <article class="min-h-screen">
    <Hero />
    <Background />
    <Projects />
  </article>
</Layout>
